<table class="table table-hover">
    <thead>
        <tr>
            <th>室分名称</th>
            <th>覆盖范围</th>
            <th>施主基站</th>
            <th>信源种类</th>
            <th>经度</th>
            <th>纬度</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: distributionList">
        <tr>
            <td data-bind="text: name"></td>
            <td data-bind="text: range"></td>
            <td data-bind="text: sourceName"></td>
            <td data-bind="text: sourceType"></td>
            <td data-bind="text: longtitute"></td>
            <td data-bind="text: lattitute"></td>
        </tr>
    </tbody>
</table>

