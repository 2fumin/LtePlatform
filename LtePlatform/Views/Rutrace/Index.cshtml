@{
    ViewBag.Title = "精确覆盖率";
}
<div class="row" ng-app="myApp" ng-controller="rutrace.root">
    <div class="col-md-4">
        @{
            Html.RenderPartial("_RutraceMenu");
        }
    </div>
    <div class="col-md-8">
        <h3 class="text-center text-primary"><strong>{{page.title}}</strong></h3>
        <div ng-view></div>
    </div>
</div>

@section Scripts{
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    @Scripts.Render("~/bundles/highcharts")
    <script src="~/Scripts/highcharts/highcharts-ng.js"></script>
    <script src="~/Scripts/service/app.url.service.js"></script>
    <script src="~/Scripts/service/app.region.service.js"></script>
    <script src="~/Scripts/service/app.kpi.service.js"></script>
    <script src="~/Scripts/service/app.parameters.service.js"></script>
    <script src="~/Scripts/mycharts/drilldown.chart.js"></script>
    <script src="~/Scripts/mycharts/drilldownPie.js"></script>
    <script src="~/Scripts/mycharts/drilldownColumn.js"></script>
    <script src="~/Scripts/mycharts/comboChart.js"></script>
    @Scripts.Render("~/bundles/ui-bootstraps")
    <script>
        var app = angular.module('myApp',
            ['ngRoute', "highcharts-ng", "ui.bootstrap", 'myApp.url', 'myApp.region', 'myApp.kpi', 'myApp.parameters']);
    </script>
    <script src="~/Scripts/rutrace/menu.controller.js"></script>
    <script src="~/Scripts/rutrace/index.controller.js"></script>
    <script src="~/Scripts/rutrace/trend.controller.js"></script>
    <script src="~/Scripts/rutrace/topcells.controller.js"></script>
    <script src="~/Scripts/rutrace/import.controller.js"></script>
    <script src="~/Scripts/rutrace/precise.chart.controller.js"></script>
    <script src="~/Scripts/rutrace/trend.chart.controller.js"></script>
    <script src="~/Scripts/rutrace/interference.controller.js"></script>
    <script src="~/Scripts/rutrace/interference.neighbors.controller.js"></script>
}