<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title">
            {{currentCell.eNodebName + "-" + currentCell.sectorId}}小区邻区列表
        </h3>
    </div>
    <div class="panel-body">
        <p>
            <div class="btn-group" style="float: right">
                <button class="btn btn-primary">更新PCI</button>
                <button class="btn btn-warning">加入监控</button>
            </div>
        </p>
        <table class="table table-hover">
            <thead>
            <tr>
                <th>小区编号</th>
                <th>扇区编号</th>
                <th>邻区小区编号</th>
                <th>邻区小区名称</th>
                <th>PCI</th>
                <th>切换次数</th>
            </tr>
            </thead>
            <tbody>
            <tr ng-repeat="neighbor in neighborCells">
                <td>{{neighbor.cellId}}</td>
                <td>{{neighbor.sectorId}}</td>
                <td>{{neighbor.nearestCellId}}</td>
                <td>{{neighbor.nearestENodebName}}-{{neighbor.nearestSectorId}}</td>
                <td>{{neighbor.pci}}</td>
                <td>{{neighbor.totalTimes}}</td>
            </tr>
            </tbody>
        </table>
    </div>
</div>

