@section SPAViews
{
    <div ng-app="myApp" ng-controller="rutrace.interference">
        @{
            Html.RenderPartial("_RutraceMenu");
        }
        <div class="row">
            <div class="col-md-6">
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <h3 class="panel-title">
                            TOP指标查询
                        </h3>
                    </div>
                    <div class="panel-body">
                        <div class="form-group">
                            <label class="col-md-2 control-label">{{beginDate.title}}</label>
                            <div datetimepicker ng-model="beginDate.value"></div>
                            <button class="btn btn-warning" ng-click="monitorAll()">全部监控</button>
                        </div>
                        <div class="form-group">
                            <label class="col-md-2 control-label">{{endDate.title}}</label>
                            <div datetimepicker ng-model="endDate.value"></div>
                            <button class="btn btn-primary" ng-click="query()">查询</button>
                        </div>
                        @{
                            Html.RenderPartial("_TopPreciseTable");
                        }
                    </div>
                </div>
            </div>
            <div class="col-md-6"></div>
        </div>
    </div>
}

@section Scripts
{
    <script src="~/Scripts/angular.js"></script>
    <script>
        var app = angular.module('myApp', []);
    </script>
    <script src="~/Scripts/rutrace/interference.controller.js"></script>
    <script src="~/Scripts/rutrace/menu.controller.js"></script>
}