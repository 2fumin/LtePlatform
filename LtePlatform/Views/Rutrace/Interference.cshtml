@section SPAViews
{
    <div ng-app="myApp" ng-controller="rutrace.interference">
        @{
            Html.RenderPartial("_RutraceMenu");
        }
        <div class="row">
            <div class="col-md-6" ng-controller="rutrace.topcells">
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <h3 class="panel-title">
                            {{cellPanelTitle}}
                        </h3>
                    </div>
                </div>
                <div class="panel-body">
                    <div class="form-group">
                        <label class="col-md-3 control-label">{{beginDate.title}}</label>
                        <div datetimepicker ng-model="beginDate.value" class="col-md-7"></div>
                        <div class="col-md-2"></div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">{{endDate.title}}</label>
                        <div datetimepicker ng-model="endDate.value" class="col-md-7"></div>
                        <div class="col-md-2">
                            <button class="btn btn-primary" ng-click="query()">查询</button>
                        </div>
                    </div>
                    @{
                        Html.RenderPartial("_TopPreciseTable");
                    }
                </div>
            </div>
            <div class="col-md-6" ng-controller="rutrace.interference.neighbors">
                @{
                    Html.RenderPartial("_InterferenceTable");
                    Html.RenderPartial("_MatchCellDialog");
                }
            </div>
        </div>
    </div>
}

@section Scripts
{
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    @Scripts.Render("~/bundles/ui-bootstraps")
    <link href="~/Content/themes/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    <script src="~/Scripts/angular-ui/bootstrap-datetimepicker.min.js"></script>
    <script src="~/Scripts/angular-ui/local/bootstrap-datetimepicker.zh-CN.js"></script>
    <script src="~/Scripts/app/common.js"></script>
    <script src="~/Scripts/service/app.url.service.js"></script>
    <script src="~/Scripts/baidu/cloud/geography.map.js"></script>
    <script>
        var app = angular.module('myApp', ['ngRoute', 'myApp.Services', "ui.bootstrap"]);
    </script>
    <script src="~/Scripts/rutrace/interference.route.js"></script>
    <script src="~/Scripts/rutrace/interference.controller.js"></script>
    <script src="~/Scripts/rutrace/topcells.controller.js"></script>
    <script src="~/Scripts/rutrace/interference.neighbors.controller.js"></script>
    <script src="~/Scripts/rutrace/menu.controller.js"></script>
    <script src="~/Scripts/angular-ui/customize/directive.datetimepicker.js"></script>
    <script src="~/Scripts/angular-ui/customize/directive.dialog.js"></script>
    <script src="~/Scripts/angular-ui/customize/directive.panel.js"></script>
}