<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title">
            {{currentCell.eNodebName + "-" + currentCell.sectorId}}干扰小区列表
        </h3>
    </div>
    <div class="panel-body">
        <table class="table table-hover">
            <thead>
            <tr>
                <th>邻小区名称</th>
                <th>邻小区PCI</th>
                <th>模3干扰数</th>
                <th>模6干扰数</th>
                <th>6dB干扰数</th>
                <th>0dB干扰数</th>
                <th>总干扰水平</th>
                <th>操作</th>
            </tr>
            </thead>
            <tbody>
            <tr ng-repeat="cell in interferenceCells">
                <td>{{cell.neighborCellName}}</td>
                <td>{{cell.destPci}}</td>
                <td>{{cell.mod3Interferences | number:1}}</td>
                <td>{{cell.mod6Interferences | number:1}}</td>
                <td>{{cell.overInterferences6Db | number:1}}</td>
                <td>{{cell.overInterferences10Db | number:1}}</td>
                <td>{{cell.interferenceLevel | number:2}}</td>
                <td>
                    <button class="btn btn-sm btn-warning" ng-if="cell.neighborCellName==='未匹配小区'">
                        匹配
                    </button>
                </td>
            </tr>
            </tbody>
        </table>
    </div>
</div>