<!-- ko with: collegeMap -->
<div class="row">
    <div class="col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" data-parent="#accordion"
                       href="#queryInfo">
                        信息查询
                    </a>
                </h4>
            </div>
            <div id="queryInfo" class="panel-collapse collapse in">
                <div class="panel-body">
                    <p>
                        开始日期：<input id="BeginDate" data-bind="value: beginDate" />
                    </p>
                    <p>
                        结束日期：<input id="EndDate" data-bind="value: endDate" />
                        <button class="btn btn-primary" data-bind="click: refresh">刷新信息</button>
                    </p>
                </div>
            </div>
        </div>
        @{ 
            Html.RenderPartial("_CollegeMenu");
        }
    </div>
    <div class="col-md-8">
        <div id="all-map" style="width: 100%;height: 800px;overflow: hidden;margin:0;"></div>
    </div>
</div>
<!-- /ko -->
