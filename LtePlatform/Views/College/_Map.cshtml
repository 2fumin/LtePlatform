<!-- ko with: collegeMap -->
<div class="row">
    <div class="col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" data-parent="#accordion"
                       href="#queryInfo">
                        信息查询
                    </a>
                </h4>
            </div>
            <div id="queryInfo" class="panel-collapse collapse in">
                <div class="panel-body">
                    <div class="form-group">
                        <label class="col-md-5 control-label">开始日期：</label>
                        <div class="col-md-7">
                            <input id="BeginDate" class="form-control" data-bind="value: beginDate" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-5 control-label">结束日期：</label>
                        <div class="col-md-7">
                            <input id="EndDate" class="form-control" data-bind="value: endDate" />
                        </div>
                    </div>
                    <div class="btn-group">
                        <button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
                            测试
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="#" data-bind="click: showRates">速率</a></li>
                            <li><a href="#" data-bind="click: showUsers">用户数</a></li>
                            <li><a href="#" data-bind="click: showCoverage">覆盖</a></li>
                            <li><a href="#" data-bind="click: showInterference">干扰</a></li>
                        </ul>
                    </div>
                    <div class="btn-group">
                        <button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
                            指标
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="#" data-bind="click: showAlarms">告警分析</a></li>
                            <li><a href="#" data-bind="click: showFlows">流量</a></li>
                            <li><a href="#" data-bind="click: showConnection">连接性能</a></li>
                            <li><a href="#" data-bind="click: showDrop">保持性能</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-8">
        <div id="all-map" style="width: 100%;height: 600px;overflow: hidden;margin:0;"></div>
        <div id="time-control">
            <div id="time-control-btns">
                <button class="time-control-btn" data-bind="click: toggleCollegeMarkers">校园标记</button>
                <span class="cloudmap-city-name">
                    LTE
                    <button class="time-control-btn" data-bind="click: toggleCollegeENodebs">基站</button>
                    <button class="time-control-btn" data-bind="click: toggleCollegeCells">小区</button>
                    <button class="time-control-btn" data-bind="click: toggleCollegeLteDistributions">室分</button>
                </span>
                <span class="cloudmap-city-name">
                    CDMA
                    <button class="time-control-btn" data-bind="click: toggleCollegeBtss">基站</button>
                    <button class="time-control-btn" data-bind="click: toggleCollegeCdmaCells">小区</button>
                    <button class="time-control-btn" data-bind="click: toggleCollegeCdmaDistributions">室分</button>
                </span>
            </div>
        </div>
    </div>
</div>
<!-- /ko -->
