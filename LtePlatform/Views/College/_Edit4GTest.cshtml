<div class="modal fade" id="edit-4G">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" data-bind="text: selectedCollege"></h4>-4G测试信息
            </div>
            <div class="modal-body form-horizontal" data-bind="with: edit4G">
                <div class="form-group">
                    <label class="col-sm-2 control-label">测试时间：</label>
                    <div class="col-sm-10">
                        <span class="form-control" data-bind="text: testTime"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="downloadRate" class="col-sm-2 control-label">下载速率（kBps）：</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="downloadRate" data-bind="value: downloadRate" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="uploadRate" class="col-sm-2 control-label">上传速率（kBps）：</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="uploadRate" data-bind="value: uploadRate" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="accessUsers" class="col-sm-2 control-label">接入用户数：</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="accessUsers" data-bind="value: accessUsers" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="rsrp" class="col-sm-2 control-label">RSRP：</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="rsrp" data-bind="value: rsrp" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="sinr" class="col-sm-2 control-label">SINR：</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="sinr" data-bind="value: sinr" />
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-default" data-dismiss="modal">关闭</button>
                <button class="btn btn-primary" data-bind="click: postTest4G">保存</button>
            </div>
        </div>
    </div>
</div>
