@{
    ViewBag.Title = "干扰矩阵数据导入";
}

<div class="panel panel-warning">
    <div class="panel-heading">
        <h2 class="panel-title">
            <strong>@ViewBag.Title</strong>
        </h2>
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-6">
                @{
                    Html.RenderPartial("_AlertPanel");
                    using (Html.BeginForm("InterferencePost", "Kpi",
                        FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal", role = "form" }))
                    {
                        <div class="form-group">
                            <label class="col-md-4 control-label">干扰矩阵数据导入：</label>
                            <div class="col-md-6">
                                <input name="files" class="form-control" type="file" multiple accept=".txt" />
                            </div>
                            <div class="col-md-2">
                                <button type="submit" class="btn btn-primary">
                                    <span class="glyphicon glyphicon-save"></span>导入
                                </button>
                            </div>
                        </div>
                    }
                }
            </div>
            <div class="col-md-6">
                <div ng-include="'/appViews/FromTxt.html'"></div>
            </div>
        </div>
    </div>
</div>



@section Scripts{
    <script src="~/Scripts/angular-ui/customize/directive.datetimepicker.js"></script>
    <script src="~/Scripts/angular-ui/customize/dump.progress.factory.js"></script>
    <script src="~/Scripts/angular-ui/customize/directive.panel.js"></script>
    <script src="~/Scripts/kpi/interference.import.controller.js"></script>
}
