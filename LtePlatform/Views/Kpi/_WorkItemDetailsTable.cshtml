<table class="table table-striped">
    <thead>
        <tr>
            <th style="width: 100px">项目</th>
            <th>信息</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>工单类型</td>
            <td>{{currentView.workItemType}}</td>
        </tr>
        <tr>
            <td>工单子类型</td>
            <td>{{currentView.workItemSubType}}</td>
        </tr>
        <tr>
            <td>基站编号</td>
            <td>
                {{currentView.eNodebId}}
                <button class="btn btn-primary btn-xs" style="float: right" ng-click="queryBtsInfo()"
                        ng-if="eNodebId > 0">
                    基站信息
                </button>
            </td>
        </tr>
        <tr>
            <td>扇区编号</td>
            <td>
                {{currentView.sectorId}}
                <button class="btn btn-primary btn-xs" style="float: right" ng-click="queryCellInfo()"
                        ng-if="eNodebId > 0">
                    小区信息
                </button>
            </td>
        </tr>
        <tr>
            <td>区域信息</td>
            <td>
                {{currentView.city}}
                {{currentView.district}}
                {{currentView.town}}
            </td>
        </tr>
        <tr>
            <td>派单时间</td>
            <td>{{currentView.beginTime}}</td>
        </tr>
        <tr>
            <td>回单期限</td>
            <td>{{currentView.deadline}}</td>
        </tr>
        <tr>
            <td>重复信息</td>
            <td>
                重复派单次数：{{currentView.repeatTimes}}
                ；驳回次数：{{currentView.repeatTimes}}
            </td>
        </tr>
        <tr>
            <td>责任人</td>
            <td>{{currentView.staffName}}</td>
        </tr>
        <tr>
            <td>反馈时间</td>
            <td>{{currentView.feedbackTime}}</td>
        </tr>
        <tr>
            <td>完成时间</td>
            <td>{{currentView.finishTime}}</td>
        </tr>
        <tr>
            <td>定位原因</td>
            <td>{{currentView.workItemCause}}</td>
        </tr>
        <tr>
            <td>工单状态</td>
            <td>{{currentView.workItemState}}</td>
        </tr>
        <tr>
            <td>平台信息</td>
            <td>{{currentView.comments}}</td>
        </tr>
        <tr>
            <td>反馈信息</td>
            <td>{{currentView.feedbackContents}}</td>
        </tr>
    </tbody>
</table>
