<!-- ko with: basicImport -->
<div class="alert alert-success" data-bind="visible: dumpResultMessage() !== ''">
    <a href="#" class="close" data-dismiss="alert">
        &times;
    </a>
    <strong>
        <span data-bind="text: dumpResultMessage"></span>
    </strong>
</div>
<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title">
            工参读取结果及操作平台
        </h3>
    </div>
    <div class="panel-body">
        <p>
            <button class="btn btn-primary" data-bind="click: postAll">一键导入</button>
        </p>
        <table class="table table-hover">
            <thead>
            <tr>
                <th>项目</th>
                <th>新增数量</th>
                <th>查看调整</th>
                <th>导入选择</th>
            </tr>
            </thead>
            <tbody>
            <tr>
                <td>LTE基站</td>
                <td data-bind="text: newENodebs().length"></td>
                <td>
                    <div class="btn-group" data-bind="visible: newENodebs().length > 0">
                        <button class="btn btn-warning" data-bind="click: checkENodebsLonLat">检查经纬度</button>
                        <button class="btn btn-warning">匹配镇区</button>
                    </div>
                </td>
                <td><input type="checkbox" data-bind="checked: newENodebsImport"/></td>
            </tr>
            <tr>
                <td>LTE小区</td>
                <td data-bind="text: newCells().length"></td>
                <td>
                    <div class="btn-group" data-bind="visible: newCells().length > 0">
                        <button class="btn btn-warning" data-bind="click: checkCellsLonLat">检查经纬度</button>
                    </div>
                </td>
                <td><input type="checkbox" data-bind="checked: newCellsImport"/></td>
            </tr>
            <tr>
                <td>CDMA基站</td>
                <td data-bind="text: newBtss().length"></td>
                <td>
                    <div class="btn-group" data-bind="visible: newBtss().length > 0">
                        <button class="btn btn-warning" data-bind="click: checkBtssLonLat">检查经纬度</button>
                    </div>
                </td>
                <td><input type="checkbox" data-bind="checked: newBtssImport"/></td>
            </tr>
            <tr>
                <td>CDMA小区</td>
                <td data-bind="text: newCdmaCells().length"></td>
                <td>
                    <div class="btn-group" data-bind="visible: newCdmaCells().length > 0">
                        <button class="btn btn-warning" data-bind="click: checkCdmaCellsLonLat">检查经纬度</button>
                    </div>
                </td>
                <td><input type="checkbox" data-bind="checked: newCdmaCellsImport"/></td>
            </tr>
            </tbody>
        </table>
    </div>
</div>
@{
    Html.RenderPartial("_ENodebLonLatTable");
    Html.RenderPartial("_CellLonLatTable");
    Html.RenderPartial("_BtsLonLatTable");
    Html.RenderPartial("_CdmaCellLonLatTable");
}
<!-- /ko -->
