<div class="modal fade" id="editCell">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">编辑LTE小区信息</h4>
            </div>
            <div class="modal-body form-horizontal" data-bind="with: editCell">
                <div class="form-group">
                    <label class="col-sm-3 control-label">基站编号</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: eNodebId"></span>
                    </div>
                    <label class="col-sm-3 control-label">频点</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: frequency"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">扇区编号</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: sectorId"></span>
                    </div>
                    <label class="col-sm-3 control-label">频段号</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: bandClass"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">室内小区</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: isIndoor"></span>
                    </div>
                    <label class="col-sm-3 control-label">天线挂高</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: height"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">经度</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: longtitute"></span>
                    </div>
                    <label class="col-sm-3 control-label">纬度</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: lattitute"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">PCI</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: pci"></span>
                    </div>
                    <label class="col-sm-3 control-label">根序列</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: prach"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">方位角</label>
                    <div class="col-sm-3">
                        <input class="form-control" data-bind="value: azimuth" />
                    </div>
                    <label class="col-sm-3 control-label">天线增益</label>
                    <div class="col-sm-3">
                        <input class="form-control" data-bind="value: antennaGain" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">机械下倾</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: mTilt"></span>
                    </div>
                    <label class="col-sm-3 control-label">电下倾角</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: eTilt"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">参考信号功率</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: rsPower"></span>
                    </div>
                    <label class="col-sm-3 control-label">收发类型</label>
                    <div class="col-sm-3">
                        <span class="form-control" data-bind="text: transmitReceive"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">天线信息</label>
                    <div class="col-sm-9">
                        <span class="form-control" data-bind="text: antennaInfo"></span>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-default" data-dismiss="modal">关闭</button>
                <button class="btn btn-primary" data-bind="click: saveCell">保存</button>
            </div>
        </div>
    </div>
</div>

