<!-- ko with: queryMap -->
<div class="cloudmap-wrapper">
    <div class="cloudmap-tools">
        <span class="cloudmap-city-name">选择网络</span>
        <select class="cloudmap-select-city" data-bind="value: currentNetwork, options: networks"></select>
        <span class="cloudmap-city-name">城市</span>
        <select class="cloudmap-select-city" data-bind="value: currentCity, options: cities"></select>
        <span class="cloudmap-city-name">区域</span>
        <select class="cloudmap-select-city" data-bind="value: currentDistrict, options: districts"></select>
        <span class="cloudmap-city-name">镇区</span>
        <select class="cloudmap-select-city" data-bind="value: currentTown, options: towns"></select>
        <span class="cloudmap-city-name">模糊查询</span>
        <input class="cloudmap-search-city" data-bind="value: queryText"/>
        <button class="cloudmap-search-b" data-bind="click: queryItems"></button>
    </div>
    <div class="cloudmap-baiduwrapper">
        <div id="mapContent" style="width: 100%;height: 600px;overflow: hidden;margin:0;"></div>
        <div class="map-w">
            <ul id="colorBar">
                <li class="map-w-1"></li>
                <li class="map-w-i"></li>
            </ul>
        </div>
        <div id="time-control">
            <div id="time-control-btns">
                <button class="time-control-btn">模3染色</button>
                <button class="time-control-btn">邻区关系</button>
            </div>
        </div>
    </div>
</div>
<!-- /ko -->

