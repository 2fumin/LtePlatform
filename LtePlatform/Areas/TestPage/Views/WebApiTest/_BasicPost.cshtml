<!-- ko with: basicPost -->
<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">
            源基站信息
        </h3>
        <button class="btn btn-primary" data-bind="click: postOrigin">上传</button>
    </div>
    <div class="panel-body">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>基站名称</th>
                    <th>基站编号</th>
                    <th>基站地址</th>
                    <th>IP</th>
                    <th>区域</th>
                    <th>镇区</th>
                </tr>
            </thead>
            <tbody data-bind="foreach: originENodebs">
                <tr>
                    <td data-bind="text: name"></td>
                    <td data-bind="text: eNodebId"></td>
                    <td data-bind="text: address"></td>
                    <td data-bind="text: ipString"></td>
                    <td data-bind="text: districtName"></td>
                    <td data-bind="text: townName"></td>
                </tr>
            </tbody>
        </table>
    </div>
    <div class="panel-heading">
        <h3 class="panel-title">
            下发基站信息
        </h3>
    </div>
    <div class="panel-body">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>基站名称</th>
                    <th>基站编号</th>
                    <th>基站地址</th>
                    <th>IP</th>
                    <th>区域</th>
                    <th>镇区</th>
                </tr>
            </thead>
            <tbody data-bind="foreach: secondENodebs">
                <tr>
                    <td data-bind="text: name"></td>
                    <td data-bind="text: eNodebId"></td>
                    <td data-bind="text: address"></td>
                    <td data-bind="text: ipString"></td>
                    <td data-bind="text: districtName"></td>
                    <td data-bind="text: townName"></td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<!-- /ko -->
